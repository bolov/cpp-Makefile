# Batch build
# calls Makefile with all builds

MAKEFLAGS += --no-print-directory

build:
	$(MAKE) -f Makefile build build=debug
	$(MAKE) -f Makefile build build=release
.PHONY:build

clean:
	$(MAKE) -f Makefile clean build=debug
	$(MAKE) -f Makefile clean build=release
